---
    import Layout from "../layouts/Layout.astro";
    import Nav from "../components/Nav.astro";
    import Footer from "../components/Footer.astro";
---

<Layout title="CV">
    <Nav />
    <main class="flex flex-col flex-wrap justify-center items-center p-4">
        <img class="w-96 h-96 object-contain rounded-3xl" src="https://i.giphy.com/media/v1.Y2lkPTc5MGI3NjExbGx2N2Y4OGV4dGllc2F2NnRrMmZtMnowczkxemJxcXFmcHMyN3VhZSZlcD12MV9pbnRlcm5hbF9naWZfYnlfaWQmY3Q9Zw/UTek0q3N8osh8agH4Y/giphy.gif" alt="">
        <p class="text-5xl font-bold">Estamos en construccion!</p>
        <a href="/" class="w-full lg:w-auto my-4 block text-center border rounded-md px-1 sm:px-16 py-5 bg-indigo-600 text-white hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-indigo-700 focus:ring-opacity-50 mt-10">Regresar al inicio</a>

    </main>

    <Footer/>
</Layout>

